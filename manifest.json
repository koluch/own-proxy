{
  "manifest_version": 2,
  "name": "Own-proxy",
  "description": "Simple extension to support proxy connecting vie authorizes SOCKS5",
  "version": "1.0",

  "icons": {
    "48": "icons/block.svg",
    "96": "icons/block.svg"
  },

  "browser_action": {
    "default_icon": {
      "19": "icons/block.svg",
      "38": "icons/block.svg"
    },
    "default_title": "Whereami?",
    "default_popup": "popups/main_popup.html"
  },

  "applications": {
    "gecko": {
      "strict_min_version": "56.0a1"
    }
  },

  "background": {
    "scripts": [
      "background/proxy-handler.js"
    ]
  },

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },

  "permissions": ["proxy", "storage", "<all_urls>"]

}
